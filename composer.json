{
  "name": "p3k/xray",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/aaronpk/XRay",
  "description": "X-Ray returns structured data from any URL",
  "require": {
    "mf2/mf2": "~0.3",
    "ezyang/htmlpurifier": "4.*",
    "indieweb/link-rel-parser": "0.1.*",
    "dg/twitter-php": "3.6.*",
    "p3k/timezone": "*",
    "p3k/http": "0.1.*",
    "cebe/markdown": "1.1.*"
  },
  "autoload": {
